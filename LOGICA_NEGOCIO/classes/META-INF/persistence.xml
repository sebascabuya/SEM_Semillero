<?xml version="1.0" encoding="windows-1252" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="SEMILLERO_JPA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <jta-data-source>DSSemillero</jta-data-source>
        <mapping-file>META-INF/orm.xml</mapping-file>
        <class>com.bancodeoccidente.semillero.modulos.entidades.ClientesEntity</class>
        <class>com.bancodeoccidente.semillero.modulos.entidades.ClavesEntity</class>
        <class>com.bancodeoccidente.semillero.modulos.entidades.ClavesInvalidasEntity</class>
        <class>com.bancodeoccidente.semillero.modulos.entidades.CuentasEntity</class>
        <class>com.bancodeoccidente.semillero.modulos.entidades.OficinasEntity</class>
        <class>com.bancodeoccidente.semillero.modulos.entidades.ProductosEntity</class>
        <class>com.bancodeoccidente.semillero.modulos.entidades.ReferenciasEntity</class>
        <class>com.bancodeoccidente.semillero.modulos.entidades.CiudadesEntity</class>
        <class>com.bancodeoccidente.semillero.modulos.entidades.DepartamentosEntity</class>
        <properties>
            <!-- DESHABILITAR CACHE -->
            <property name="eclipselink.cache.shared.default" value="false"/>
            <!-- FIN COMENTARIO -->
        </properties>
    </persistence-unit>
</persistence>
